{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Gray",
	"description":  "Gray-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#374151", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #374151 0%, #6b7280 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #6b7280 0%, #374151 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #d1d5db 0%, #f3f4f6 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #f3f4f6 0%, #d1d5db 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#374151",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#6b7280",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#d1d5db",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f3f4f6",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}