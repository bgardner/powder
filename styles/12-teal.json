{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Teal",
	"description":  "Teal-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#0f766e", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #0f766e 0%, #14b8a6 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #14b8a6 0%, #0f766e 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #5eead4 0%, #ccfbf1 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #ccfbf1 0%, #5eead4 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#0f766e",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#14b8a6",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#5eead4",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#ccfbf1",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}