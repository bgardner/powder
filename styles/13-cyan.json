{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Cyan",
	"description":  "Cyan-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#0e7490", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #0e7490 0%, #06b6d4 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #06b6d4 0%, #0e7490 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #67e8f9 0%, #cffafe 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #cffafe 0%, #67e8f9 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#0e7490",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#06b6d4",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#67e8f9",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#cffafe",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}