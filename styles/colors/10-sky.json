{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Sky",
	"description": "Sky-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#024a70", "#ffffff" ],
