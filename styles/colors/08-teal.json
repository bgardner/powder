{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Teal",
	"description": "Teal-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0b4f4a", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				},
				{
					"colors": [ "#009689", "#ffffff" ],
					"name": "Secondary and White",
					"slug": "secondary-and-white"
				},
				{
					"colors": [ "#000000", "#ffffff" ],
					"name": "Contrast and White",
					"slug": "contrast-and-white"
				},
				{
					"colors": [ "#000000", "#46ecd5" ],
					"name": "Contrast and Tertiary",
					"slug": "contrast-and-tertiary"
				},
				{
					"colors": [ "#000000", "#cbfbf1" ],
					"name": "Contrast and Quaternary",
					"slug": "contrast-and-quaternary"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(0deg, #0b4f4a 0%, #000000 100%)",
					"name": "Primary to Contrast",
					"slug": "primary-contrast"
				}
			],
			"palette": [
				{
					"color": "#0b4f4a",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#009689",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#46ecd5",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#cbfbf1",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		}
	}
}