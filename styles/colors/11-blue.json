{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Blue",
	"description": "Blue-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#1c398e", "#ffffff" ],
