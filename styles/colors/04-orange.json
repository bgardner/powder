{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Orange",
	"description": "Orange-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#7e2a0c", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				},
				{
					"colors": [ "#f54a00", "#ffffff" ],
					"name": "Secondary and White",
					"slug": "secondary-and-white"
				},
				{
					"colors": [ "#000000", "#ffffff" ],
					"name": "Contrast and White",
					"slug": "contrast-and-white"
				},
				{
					"colors": [ "#000000", "#ffb86a" ],
					"name": "Contrast and Tertiary",
					"slug": "contrast-and-tertiary"
				},
				{
					"colors": [ "#000000", "#ffedd4" ],
					"name": "Contrast and Quaternary",
					"slug": "contrast-and-quaternary"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(0deg, #7e2a0c 0%, #000000 100%)",
					"name": "Primary to Contrast",
					"slug": "primary-contrast"
				}
			],
			"palette": [
				{
					"color": "#7e2a0c",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#f54a00",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#ffb86a",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#ffedd4",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		}
	}
}