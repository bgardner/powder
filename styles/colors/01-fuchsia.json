{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Fuchsia",
	"description": "Fuchsia-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#721378", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				},
				{
					"colors": [ "#c800de", "#ffffff" ],
					"name": "Secondary and White",
					"slug": "secondary-and-white"
				},
				{
					"colors": [ "#000000", "#ffffff" ],
					"name": "Contrast and White",
					"slug": "contrast-and-white"
				},
				{
					"colors": [ "#000000", "#f4a8ff" ],
					"name": "Contrast and Tertiary",
					"slug": "contrast-and-tertiary"
				},
				{
					"colors": [ "#000000", "#fae8ff" ],
					"name": "Contrast and Quaternary",
					"slug": "contrast-and-quaternary"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(0deg, #721378 0%, #000000 100%)",
					"name": "Primary to Contrast",
					"slug": "primary-contrast"
				}
			],
			"palette": [
				{
					"color": "#721378",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#c800de",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#f4a8ff",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#fae8ff",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		}
	}
}