{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Cyan",
	"description": "Cyan-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#104e64", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				},
				{
					"colors": [ "#0092b8", "#ffffff" ],
					"name": "Secondary and White",
					"slug": "secondary-and-white"
				},
				{
					"colors": [ "#000000", "#ffffff" ],
					"name": "Contrast and White",
					"slug": "contrast-and-white"
				},
				{
					"colors": [ "#000000", "#53eafd" ],
					"name": "Contrast and Tertiary",
					"slug": "contrast-and-tertiary"
				},
				{
					"colors": [ "#000000", "#cefafe" ],
					"name": "Contrast and Quaternary",
					"slug": "contrast-and-quaternary"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(0deg, #104e64 0%, #000000 100%)",
					"name": "Primary to Contrast",
					"slug": "primary-contrast"
				}
			],
			"palette": [
				{
					"color": "#104e64",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#0092b8",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#53eafd",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#cefafe",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		}
	}
}