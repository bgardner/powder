{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Fuchsia",
	"description":  "Fuchsia-based palette with three shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#ffffff", "#ffffff" ],
					"name": "White and White",
					"slug": "white-and-white"
				},
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Contrast and White",
					"slug": "contrast-and-white"
				},
				{
					"colors": [ "#701a75", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				},
				{
					"colors": [ "#a21caf", "#ffffff" ],
					"name": "Secondary and White",
					"slug": "secondary-and-white"
				},
				{
					"colors": [ "#d946ef", "#ffffff" ],
					"name": "Tertiary and White",
					"slug": "tertiary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(var(--wp--preset--color--base) 50%, var(--wp--preset--color--contrast) 50%)",
					"name": "Base to Contrast",
					"slug": "base-contrast"
				},
				{
					"gradient": "linear-gradient(var(--wp--preset--color--contrast) 50%, var(--wp--preset--color--base) 50%)",
					"name": "Contrast to Base",
					"slug": "contrast-base"
				},
				{
					"gradient": "linear-gradient(180deg, #701a75 0%, #a21caf 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #a21caf 0%, #701a75 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#701a75",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#a21caf",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#d946ef",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f5f5f5",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		}
	}
}