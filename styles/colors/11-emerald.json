{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Emerald",
	"description":  "Emerald-based palette with three shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Contrast and White",
					"slug": "contrast-and-white"
				},
				{
					"colors": [ "#064e3b", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				},
				{
					"colors": [ "#047857", "#ffffff" ],
					"name": "Secondary and White",
					"slug": "secondary-and-white"
				},
				{
					"colors": [ "#10b981", "#ffffff" ],
					"name": "Tertiary and White",
					"slug": "tertiary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #064e3b 0%, #047857 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #047857 0%, #064e3b 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#064e3b",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#047857",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#10b981",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f5f5f5",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		}
	}
}