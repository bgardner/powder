{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Gray",
	"description":  "Gray-based palette with three shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#ffffff", "#ffffff" ],
					"name": "White and White",
					"slug": "white-and-white"
				},
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#374151", "#ffffff" ],
					"name": "Contrast and White",
					"slug": "contrast-and-white"
				},
				{
					"colors": [ "#4b5563", "#ffffff" ],
					"name": "Secondary and White",
					"slug": "secondary-and-white"
				},
				{
					"colors": [ "#6b7280", "#ffffff" ],
					"name": "Tertiary and White",
					"slug": "tertiary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(var(--wp--preset--color--base) 50%, var(--wp--preset--color--contrast) 50%)",
					"name": "Base to Contrast",
					"slug": "base-contrast"
				},
				{
					"gradient": "linear-gradient(var(--wp--preset--color--contrast) 50%, var(--wp--preset--color--base) 50%)",
					"name": "Contrast to Base",
					"slug": "contrast-base"
				},
				{
					"gradient": "linear-gradient(180deg, #374151 0%, #4b5563 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #4b5563 0%, #374151 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#374151",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#4b5563",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#6b7280",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f5f5f5",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		}
	}
}