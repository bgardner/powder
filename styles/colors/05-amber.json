{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Amber",
	"description": "Amber-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#7b3306", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				},
				{
					"colors": [ "#e17100", "#ffffff" ],
					"name": "Secondary and White",
					"slug": "secondary-and-white"
				},
				{
					"colors": [ "#000000", "#ffffff" ],
					"name": "Contrast and White",
					"slug": "contrast-and-white"
				},
				{
					"colors": [ "#000000", "#ffd230" ],
					"name": "Contrast and Tertiary",
					"slug": "contrast-and-tertiary"
				},
				{
					"colors": [ "#000000", "#fef3c6" ],
					"name": "Contrast and Quaternary",
					"slug": "contrast-and-quaternary"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(0deg, #7b3306 0%, #000000 100%)",
					"name": "Primary to Contrast",
					"slug": "primary-contrast"
				}
			],
			"palette": [
				{
					"color": "#7b3306",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#e17100",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#ffd230",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#fef3c6",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		}
	}
}