{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Sky",
	"description":  "Sky-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#0369a1", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #0369a1 0%, #0ea5e9 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #0ea5e9 0%, #0369a1 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #7dd3fc 0%, #e0f2fe 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #e0f2fe 0%, #7dd3fc 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#0369a1",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#0ea5e9",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#7dd3fc",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#e0f2fe",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}