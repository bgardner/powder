{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Slate",
	"description":  "Slate-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#334155", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #334155 0%, #64748b 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #64748b 0%, #334155 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #64748b 0%, #f1f5f9 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #f1f5f9 0%, #64748b 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#334155",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#64748b",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#cbd5e1",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f1f5f9",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}