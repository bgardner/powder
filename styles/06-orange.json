{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Orange",
	"description":  "Orange-based palette with three shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#7c2d12", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #7c2d12 0%, #c2410c 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #c2410c 0%, #7c2d12 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#7c2d12",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#c2410c",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#f97316",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f5f5f5",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		}
	}
}