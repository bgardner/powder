{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Zinc",
	"description":  "Zinc-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#3f3f46", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #3f3f46 0%, #71717a 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #71717a 0%, #3f3f46 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #d4d4d8 0%, #f4f4f5 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #f4f4f5 0%, #d4d4d8 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#3f3f46",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#71717a",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#d4d4d8",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f4f4f5",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}