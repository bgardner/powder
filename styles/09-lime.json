{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Lime",
	"description":  "Lime-based palette with three shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#365314", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #365314 0%, #4d7c0f 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #4d7c0f 0%, #365314 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#365314",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#4d7c0f",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#84cc16",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f5f5f5",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		}
	}
}