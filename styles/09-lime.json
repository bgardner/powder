{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Lime",
	"description":  "Lime-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#4d7c0f", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #4d7c0f 0%, #84cc16 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #84cc16 0%, #4d7c0f 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #bef264 0%, #ecfccb 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #ecfccb 0%, #bef264 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#4d7c0f",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#84cc16",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#bef264",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#ecfccb",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}