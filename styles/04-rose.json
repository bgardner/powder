{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Rose",
	"description":  "Rose-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#be123c", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #be123c 0%, #f43f5e 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #f43f5e 0%, #be123c 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #fda4af 0%, #ffe4e6 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #ffe4e6 0%, #fda4af 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#be123c",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#f43f5e",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#fda4af",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#ffe4e6",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}