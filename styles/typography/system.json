{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "System",
	"description": "System font stack",
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, system-ui, sans-serif",
					"name": "System",
					"slug": "system"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "var(--wp--preset--font-size--small)"
		}
	}
}