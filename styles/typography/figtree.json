{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Figtree",
	"description":  "Figtree-based typography",
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--figtree)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "var(--wp--custom--font-weight--regular)"
		}
	}
}