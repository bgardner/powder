{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Indigo",
	"description":  "Indigo-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#4338ca", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #4338ca 0%, #6366f1 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #6366f1 0%, #4338ca 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #a5b4fc 0%, #e0e7ff 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #e0e7ff 0%, #a5b4fc 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#4338ca",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#6366f1",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#a5b4fc",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#e0e7ff",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}