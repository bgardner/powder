{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Yellow",
	"description":  "Yellow-based palette with three shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#713f12", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #713f12 0%, #a16207 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #a16207 0%, #713f12 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#713f12",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#a16207",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#eab308",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#f5f5f5",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		}
	}
}