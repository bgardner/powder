{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Green",
	"description":  "Green-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#15803d", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #15803d 0%, #22c55e 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #22c55e 0%, #15803d 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#15803d",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#22c55e",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#86efac",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#dcfce7",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
