{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Fuchsia",
	"description":  "Fuchsia-based palette with four shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [ "#0a0a0a", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#a21caf", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, #a21caf 0%, #d946ef 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(180deg, #d946ef 0%, #a21caf 100%)",
					"name": "Secondary to Primary",
					"slug": "secondary-primary"
				},
				{
					"gradient": "linear-gradient(180deg, #f0abfc 0%, #fae8ff 100%)",
					"name": "Tertiary to Quaternary",
					"slug": "tertiary-quaternary"
				},
				{
					"gradient": "linear-gradient(180deg, #fae8ff 0%, #f0abfc 100%)",
					"name": "Quaternary to Tertiary",
					"slug": "quaternary-tertiary"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#a21caf",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#d946ef",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#f0abfc",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#fae8ff",
					"name": "Quaternary",
					"slug": "quaternary"
				}
			]
		}
	}
}