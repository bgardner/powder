{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/button": {
				"spacing": {
					"spacingSizes": [
						{
							"name": "xSmall",
							"size": "10px",
							"slug": "x-small"
						},
						{
							"name": "Small",
							"size": "15px",
							"slug": "small"
						},
						{
							"name": "Medium",
							"size": "20px",
							"slug": "medium"
						},
						{
							"name": "Large",
							"size": "30px",
							"slug": "large"
						},
						{
							"name": "xLarge",
							"size": "40px",
							"slug": "x-large"
						}
					]
				}
			}
		},
		"border": {
			"radiusSizes": [
				{
					"name": "xSmall",
					"size": "4px",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "8px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "12px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "20px",
					"slug": "large"
				},
				{
					"name": "xLarge",
					"size": "40px",
					"slug": "x-large"
				}
			]
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [ "#000000", "#ffffff" ],
					"name": "Black and White",
					"slug": "black-and-white"
				},
				{
					"colors": [ "#440099", "#ffffff" ],
					"name": "Primary and White",
					"slug": "primary-and-white"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(var(--wp--preset--color--base) 50%, var(--wp--preset--color--contrast) 50%)",
					"name": "Base to Contrast",
					"slug": "base-contrast"
				},
				{
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--base) 50%, var(--wp--preset--color--contrast) 50%)",
					"name": "Base to Contrast (90)",
					"slug": "base-contrast-90"
				},
				{
					"gradient": "linear-gradient(var(--wp--preset--color--contrast) 50%, var(--wp--preset--color--base) 50%)",
					"name": "Contrast to Base",
					"slug": "contrast-base"
				},
				{
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--contrast) 50%, var(--wp--preset--color--base) 50%)",
					"name": "Contrast to Base (90)",
					"slug": "contrast-base-90"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#440099",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#f5f5f5",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		},
		"custom": {
			"lineHeight": {
				"x-small": 1.1,
				"small": 1.25,
				"medium": 1.5,
				"large": 1.75,
				"x-large": 2
			}
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1280px"
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Light",
					"shadow": "0 0 10px rgb(0, 0, 0, 0.1)",
					"slug": "light"
				},
				{
					"name": "Natural",
					"shadow": "5px 5px 10px rgb(0, 0, 0, 0.2)",
					"slug": "natural"
				},
				{
					"name": "Deep",
					"shadow": "0 0 40px rgb(0, 0, 0, 0.2)",
					"slug": "deep"
				},
				{
					"name": "Solid",
					"shadow": "5px 5px 0 currentColor",
					"slug": "solid"
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "20",
					"size": "clamp(15px, 2vw, 20px)",
					"slug": "20"
				},
				{
					"name": "30",
					"size": "clamp(25px, 3vw, 30px)",
					"slug": "30"
				},
				{
					"name": "40",
					"size": "clamp(30px, 4vw, 40px)",
					"slug": "40"
				},
				{
					"name": "50",
					"size": "clamp(35px, 5vw, 50px)",
					"slug": "50"
				},
				{
					"name": "60",
					"size": "clamp(40px, 6vw, 60px)",
					"slug": "60"
				},
				{
					"name": "70",
					"size": "clamp(50px, 7vw, 70px)",
					"slug": "70"
				},
				{
					"name": "80",
					"size": "clamp(60px, 8vw, 80px)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": false,
			"fluid": {
				"maxViewportWidth": "1280px",
				"minViewportWidth": "640px"
			},
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Google Sans",
							"fontStyle": "normal",
							"fontWeight": "100 1000",
							"src": [
								"file:./assets/fonts/google-sans/GoogleSansFlex-Variable.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Google Sans",
							"fontStyle": "italic",
							"fontWeight": "100 1000",
							"src": [
								"file:./assets/fonts/google-sans/GoogleSansFlex-Variable.woff2"
							]
						}
					],
					"fontFamily": "Google Sans, sans-serif",
					"name": "Google Sans",
					"slug": "google-sans"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Google Sans Condensed",
							"fontStretch": "75%",
							"fontStyle": "normal",
							"fontWeight": "100 1000",
							"src": [
								"file:./assets/fonts/google-sans/GoogleSansFlex-Variable.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Google Sans Condensed",
							"fontStretch": "75%",
							"fontStyle": "italic",
							"fontWeight": "100 1000",
							"src": [
								"file:./assets/fonts/google-sans/GoogleSansFlex-Variable.woff2"
							]
						}
					],
					"fontFamily": "Google Sans Condensed, sans-serif",
					"name": "Google Sans Condensed",
					"slug": "google-sans-condensed"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Google Sans Expanded",
							"fontStretch": "125%",
							"fontStyle": "normal",
							"fontWeight": "100 1000",
							"src": [
								"file:./assets/fonts/google-sans/GoogleSansFlex-Variable.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Google Sans Expanded",
							"fontStretch": "125%",
							"fontStyle": "italic",
							"fontWeight": "100 1000",
							"src": [
								"file:./assets/fonts/google-sans/GoogleSansFlex-Variable.woff2"
							]
						}
					],
					"fontFamily": "Google Sans Expanded, sans-serif",
					"name": "Google Sans Expanded",
					"slug": "google-sans-expanded"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "xxSmall",
					"size": "14px",
					"slug": "xx-small"
				},
				{
					"fluid": false,
					"name": "xSmall",
					"size": "16px",
					"slug": "x-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "18px",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "18px",
						"max": "20px"
					},
					"name": "Medium",
					"size": "20px",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "20px",
						"max": "24px"
					},
					"name": "Large",
					"size": "24px",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "24px",
						"max": "30px"
					},
					"name": "xLarge",
					"size": "30px",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "30px",
						"max": "36px"
					},
					"name": "xxLarge",
					"size": "36px",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "14px",
								"right": "29px",
								"bottom": "14px",
								"left": "29px"
							}
						}
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "10px"
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "& code {overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/heading": {
				"css": "&.is-style-balanced {text-wrap: balance;}"
			},
			"core/list": {
				"css": "&.is-style-no-style {list-style-type: none; padding-left: 0;}",
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/navigation": {
				"css": "& .wp-block-navigation__responsive-container-open, .wp-block-navigation__responsive-container-close {border: 1px solid currentColor; padding: 2px;}",
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--20)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/navigation-submenu": {
				"border": {
					"color": "currentColor !important"
				}
			},
			"core/paragraph": {
				"css": "&.is-style-balanced {text-wrap: balance;}"
			},
			"core/post-comments-form": {
				"css": " & .form-submit {margin-bottom: 0;}"
			},
			"core/post-content": {
				"css": ".is-style-indent p:not(:first-of-type) {text-indent: var(--wp--preset--spacing--20);}"
			},
			"core/post-template": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--60);"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "{overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"core/query-pagination": {
				"css": "margin-top: var(--wp--preset--spacing--30) !important;"
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/quote": {
				"border": {
					"color": "currentColor",
					"style": "solid",
					"width": "1px"
				},
				"css": "& p {margin: 0;}",
				"elements": {
					"cite": {
						"spacing": {
							"margin": {
								"top": "10px"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-small)",
							"fontStyle": "inherit"
						}
					}
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/search": {
				"css": "& .wp-block-search__input, .wp-element-button {padding: 10px 20px;} & .wp-block-search__input {border-color: var(--wp--preset--color--contrast);}"
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"radius": "0",
					"style": "solid",
					"width": "1px 0 0"
				},
				"css": ":not(.is-style-wide):not(.is-style-dots) {width: 80px;} &.is-style-extended {width: 180px;}"
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"core/social-links": {
				"css": ".is-style-outline li.wp-block-social-link {background-color: transparent;border: 1px solid currentColor;color: currentColor;}",
				"spacing": {
					"blockGap": "10px"
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"css": "html {-moz-osx-font-smoothing: grayscale; -webkit-font-smoothing: antialiased;} ::selection {background-color: var(--wp--preset--color--primary); color: var(--wp--preset--color--base);}",
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "{width: auto;}",
				"spacing": {
					"padding": {
						"top": "15px",
						"right": "30px",
						"bottom": "15px",
						"left": "30px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-small)",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				},
				":focus": {
					"outline": {
						"color": "var(--wp--preset--color--primary)",
						"offset": "1px",
						"style": "solid",
						"width": "2px"
					}
				}
			},
			"caption": {
				"spacing": {
					"margin": {
						"top": "10px",
						"bottom": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				":focus": {
					"outline": {
						"color": "currentColor",
						"offset": "1px",
						"style": "solid",
						"width": "1px"
					}
				},
				"css": "a, a:not(.wp-element-button) {text-decoration-thickness: 1px; text-underline-offset: 2px;}"
			},
			"textInput": {
				"border": {
					"color": "currentColor",
					"radius": "0",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--base)",
					"text": "inherit"
				},
				"spacing": {
					"padding": {
						"top": "10px",
						"right": "15px",
						"bottom": "15px",
						"left": "15px"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--30)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--google-sans)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "400",
			"letterSpacing": "0.01em",
			"lineHeight": "var(--wp--custom--line-height--large)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "sidebar",
			"name": "sidebar",
			"title": "Sidebar"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page"
			],
			"title": "Blank"
		},
		{
			"name": "no-title",
			"postTypes": [
				"page"
			],
			"title": "No Title"
		}
	]
}